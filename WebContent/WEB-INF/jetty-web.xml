<Configure class="org.eclipse.jetty.webapp.WebAppContext">
	<New id="DLDataSource" class="org.eclipse.jetty.plus.jndi.Resource">
		<Arg></Arg>
		<Arg>jdbc/DLDataSource</Arg>
		<Arg>
			<New class="com.mysql.cj.jdbc.MysqlConnectionPoolDataSource">
				<Set name="URL">jdbc:mysql://localhost:3306/devicelytics?serverTimezone=Europe/Rome</Set>
				<Set name="User">root</Set>
				<Set name="Password"></Set>
			</New>
		</Arg>
	</New>
</Configure>